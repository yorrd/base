{
    "name": "kickstart-meteor-polymer",
    "version": "2.0.0-alpha",
    "private": true,
    "scripts": {
        "start": "yarn install && rm -r imports/ui/node_links/@polymer && cp -r node_modules/@polymer/ imports/ui/node_links && find imports/ui/node_links/ -name \"*.js\" -exec bash -c 'mv \"$1\" \"${1%.js}\".ts' - '{}' \\; && meteor run | grep -v node_links",
        "weigh": "weigh --path imports/ui/index.html --depth 2",
        "testdeploy": "deploy/testDeploy.sh",
        "rundeploy": "deploy/start.sh",
        "upload": "deploy/upload.sh BaseProject git@adornis.de:/srv/node/BaseProjectDeploy 30"
    },
    "dependencies": {
        "@babel/runtime": "^7.0.0-beta.40",
        "@polymer/paper-button": "^3.0.0-pre.10",
        "@polymer/polymer": "^3.0.0-pre.10",
        "@types/meteor": "^1.4.13",
        "@types/redux": "^3.6.0",
        "@types/redux-devtools-extension": "^2.13.2",
        "@webcomponents/custom-elements": "github:webcomponents/custom-elements",
        "@webcomponents/webcomponentsjs": "^1.0.18",
        "core-js": "^2.5.3"
    },
    "devDependencies": {
        "polymer-weight": "^1.0.3",
        "tslint": "^5.8.0",
        "tslint-eslint-rules": "^4.1.1",
        "typescript": "^2.6.2"
    }
}
